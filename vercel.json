{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/app.py"
      },
      {
        "source": "/public/(.*)",
        "destination": "/public/:*"
      }
    ],
    "public": true
  }